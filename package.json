{
  "name": "flux2-pro-generator",
  "version": "1.0.0",
  "description": "Advanced AI image generation powered by Cloudflare Workers AI and FLUX.2",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "pages:build": "npx @opennextjs/cloudflare@latest",
    "preview": "npm run pages:build && wrangler pages dev",
    "deploy": "npm run pages:build && wrangler pages deploy",
    "kv:create": "wrangler kv:namespace create IMAGE_CACHE",
    "postinstall": "npm audit fix --force || true"
  },
  "dependencies": {
    "next": "15.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "clsx": "^2.1.0"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^0.2.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "typescript": "^5.3.0",
    "wrangler": "^3.78.0",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "overrides": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
