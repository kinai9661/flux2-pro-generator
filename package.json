{
  "name": "flux2-pro-generator",
  "version": "1.0.0",
  "description": "Advanced AI image generation powered by Cloudflare Workers AI and FLUX.2",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "pages:build": "npx @opennextjs/cloudflare@latest",
    "preview": "npm run pages:build && wrangler pages dev",
    "deploy": "npm run pages:build && wrangler pages deploy",
    "kv:create": "wrangler kv:namespace create IMAGE_CACHE"
  },
  "dependencies": {
    "next": "^15.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "clsx": "^2.1.0"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^1.0.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "typescript": "^5.6.0",
    "wrangler": "^3.87.0",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0"
  }
}
